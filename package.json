        "svelte": "^3.9.1",
        "lit-element": "^2.2.1"
    },
    "devDependencies": {
        "mini-css-extract-plugin": "^0.8.0",
        "poi": "latest",
        "prettier": "^1.18.2",
        "prettier-plugin-svelte": "^0.7.0",
        "serve": "^11.1.0",
        "style-loader": "^1.0.0",
        "svelte-loader": "^2.13.6"
    }
}
